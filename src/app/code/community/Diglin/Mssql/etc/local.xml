<config>
    <global>
        <resources>
            <default_mssql>
                <connection>
                    <host>{{db_host_erp}}</host>
                    <username>{{db_user_erp}}</username>
                    <password>{{db_pass_erp}}</password>
                    <dbname>{{db_name_erp}}</dbname>
                    <model>{{db_model_erp}}</model>
                    <initStatements>{{db_init_statements_erp}}</initStatements>
                    <type>{{db_type_erp}}</type>
                    <active>1</active>
                </connection>
            </default_mssql>
        </resources>
    </global>
</config>
